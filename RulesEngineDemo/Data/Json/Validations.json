[
  {
    "WorkflowName": "Validations",
    "Rules": [
      {
        "RuleName": "InsuranceTypeIsValid",
        "Enabled": true,
        "Expression": "new[] {\"FD\", \"XOL\",\"ETC\"}.Contains(insuranceType)",
        "ErrorMessage": "InsuranceType must be one of \"FD\", \"XOL\", \"ETC\""
      },
      {
        "RuleName": "ClaimLimitIsNotExceeded",
        "Enabled": true,
        "Expression": "claimAmount <= claimLimit",
        "ErrorMessage": "ClaimAmount must not be greater than ClaimLimit"
      },
      {
        "RuleName": "ContractTermIsValid",
        "Enabled": true,
        "Expression": "expirationDate == effectiveDate.AddMonths(termMonths)",
        "ErrorMessage": "ExpirationDate must be EffectiveDate + TermMonths"
      }
    ]
  }
]
